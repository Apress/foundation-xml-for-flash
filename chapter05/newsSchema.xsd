		<xsd:complexType>
				<xsd:sequence>
				<xsd:element name="newsDate" type="xsd:string"/>
				<xsd:element name="newsTitle" type="xsd:string"/>
				<xsd:element name="newsContent" type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
